{
  "name": "lilhermit/cakephp-plugin-bootstrap4",
  "description": "Bootstrap-4 plugin for CakePHP-3",
  "type": "cakephp-plugin",
  "require": {
    "cakephp/cakephp": "^4.0",
    "lilhermit/cakephp-plugin-toolkit": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7|^6.0",
    "ext-json": "^1.2",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "LilHermit\\Bootstrap4\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
      "TestApp\\": "tests/test_app/TestApp",
      "TestPlugin\\": "tests/test_app/Plugin/TestPlugin/src"
    }
  },
  "repositories": {
    "lilHermit/cakephp-plugins-toolkit": {
      "type": "vcs",
      "url": "git@github.com:Moutard3/cakephp-plugins-toolkit.git"
    }
  }
}
